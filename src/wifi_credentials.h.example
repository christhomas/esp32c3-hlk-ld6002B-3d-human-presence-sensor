// WiFi Credentials Configuration
// 
// Copy this file to wifi_credentials.h and update with your WiFi credentials
// The wifi_credentials.h file is in .gitignore and will not be committed to git
//
// Example:
//   cp wifi_credentials.h.example wifi_credentials.h
//   # Then edit wifi_credentials.h with your actual credentials

#ifndef WIFI_CREDENTIALS_H
#define WIFI_CREDENTIALS_H

// Replace with your WiFi network name (SSID)
#define WIFI_SSID      "YOUR_WIFI_SSID_HERE"

// Replace with your WiFi password
#define WIFI_PASSWORD  "YOUR_WIFI_PASSWORD_HERE"

#endif // WIFI_CREDENTIALS_H
